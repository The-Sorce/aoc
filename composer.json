{
    "name": "the-sorce/aoc",
    "description": "Advent of Code PHP solutions",
    "keywords": ["aoc", "puzzle", "console", "cli"],
    "homepage": "https://github.com/The-Sorce/aoc",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Tony Karlsson",
            "email": "tony.karlsson@tjk-systems.fi"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "laravel-zero/framework": "^11.36.1"
    },
    "require-dev": {
        "laravel/pint": "^1.18.3",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.7.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["aoc"],
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
